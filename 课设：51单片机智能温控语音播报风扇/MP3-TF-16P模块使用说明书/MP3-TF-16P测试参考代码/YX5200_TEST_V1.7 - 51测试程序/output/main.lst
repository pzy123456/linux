 159          void Uart_SendCMD(INT8U CMD ,INT8U feedback , INT16U dat)
 160          {
 161              Send_buf[0] = 0xff;    //保留字节 
 162              Send_buf[1] = 0x06;    //长度
 163              Send_buf[2] = CMD;     //控制指令
 164              Send_buf[3] = feedback;//是否需要反馈
 165              Send_buf[4] = (INT8U)(dat >> 8);//datah
 166              Send_buf[5] = (INT8U)(dat);     //datal
 167              DoSum(&Send_buf[0],6);        //校验
 168              SendCmd(8);       //发送此帧数据
 169          }
